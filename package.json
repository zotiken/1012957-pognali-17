{
    "name": "pognali-17",
    "version": "17.0.0",
    "private": true,
    "description": "Личный проект «Погнали» от HTML Academy",
    "devDependencies": {
        "@htmlacademy/editorconfig-cli": "1.x",
        "autoprefixer": "9.5.1",
        "browser-sync": "^2.26.7",
        "gulp": "^4.0.2",
        "gulp-csso": "^3.0.1",
        "gulp-imagemin": "^6.1.0",
        "gulp-less": "4.0.x",
        "gulp-plumber": "1.2.x",
        "gulp-postcss": "8.0.x",
        "gulp-rename": "^1.4.0",
        "gulp-sourcemaps": "2.6.x",
        "stylelint": "10.1.x",
        "stylelint-config-htmlacademy": "^0.1.4"
    },
    "scripts": {
        "editorconfig": "editorconfig-cli",
        "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
        "test": "npm run editorconfig && npm run stylelint",
        "build": "gulp build",
        "start": "gulp start"
