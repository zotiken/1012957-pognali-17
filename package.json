        "*.js",
        "source/*.html",
        "source/js/**/*.js",
        "source/img/**/*.svg",
        "source/less/**/*.less"
    ],
    "engines": {
        "node": "8.16"
    },
    "dependencies": {
        "del": "^5.1.0",
        "gulp-ext-replace": "^0.3.0",
        "gulp-posthtml": "^3.0.4",
        "gulp-svgstore": "^7.0.1",
        "imagemin-webp": "^5.1.0",
        "posthtml-include": "^1.2.0"
